{
  "all": true,
  "include": ["out/core/**/*.js", "out/services/**/*.js", "out/utils/**/*.js"],
  "exclude": ["out/test/**", "out/ui/**", "out/extension.js", "out/commands/**"],
  "reporter": ["text", "html", "lcov"],
  "report-dir": "./coverage",
  "temp-directory": "./coverage/.tmp",
  "check-coverage": true,
  "lines": 60,
  "functions": 60,
  "branches": 50,
  "statements": 60
}
